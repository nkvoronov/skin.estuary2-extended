<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="musicvideos">
    <name>Resent Unwatched MusicVideos</name>
    <match>all</match>
    <rule field="playcount" operator="lessthan">
    <value>1</value>
    </rule>
    <limit>25</limit>
    <order direction="descending">dateadded</order>
</smartplaylist>